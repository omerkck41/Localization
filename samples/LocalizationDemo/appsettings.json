{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Localization": {
    "DefaultCulture": "en-US",
    "FallbackCulture": "en-US",
    "SupportedCultures": [ "en-US", "tr-TR", "fr-FR" ],
    "UseFallbackCulture": true,
    "ThrowOnMissingResource": false,
    "EnableCaching": true,
    "CacheExpiration": "01:00:00",
    "ResourcePaths": [ "Resources" ],
    "EnableResourceFileWatching": true,
    "EnableDebugLogging": true
  }
}
